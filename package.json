    "@graphql-codegen/typescript-operations": "^2.5.2",
    "@mantine/carousel": "^5.0.0",
    "@mantine/core": "5.0.0",
    "@mantine/dates": "5.0.0",
    "@mantine/dropzone": "^5.0.0",
    "@mantine/form": "^5.0.0",
    "@mantine/hooks": "5.0.0",
    "@mantine/modals": "^5.0.0",
    "@mantine/next": "5.0.0",
    "@mantine/notifications": "5.0.0",
    "@mantine/nprogress": "^5.0.0",
    "@mantine/spotlight": "^5.0.0",
    "@modulz/radix-icons": "^4.0.0",
    "@next-auth/react-query": "^0.0.12",
    "@next/bundle-analyzer": "^12.1.4",
    "@next/swc-win32-x64-msvc": "^11.1.2",
    "@tabler/icons": "^1.78.1",
    "@tanstack/react-query": "^4.0.10",
    "amazon-cognito-identity-js": "^5.2.10",
    "aws-sdk": "^2.1185.0",
    "browser-image-compression": "^2.0.0",
    "cookies-next": "^2.0.4",
    "dayjs": "^1.11.6",
    "emoji-picker-react": "^4.3.0",
    "graphql": "^16.5.0",
    "graphql-request": "^4.3.0",
    "immer": "^9.0.16",
    "mongodb": "^4.6.0",
    "next": "12.2.3",
    "prism-react-renderer": "^1.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-lottie": "^1.2.3",
    "react-virtualized-auto-sizer": "^1.0.6",
    "react-window": "^1.8.7",
    "tabler-icons-react": "^1.48.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@faker-js/faker": "^7.3.0",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript-type-graphql": "^2.2.12",
    "@next/eslint-plugin-next": "^12.1.4",
    "@testing-library/dom": "^8.12.0",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^14.0.4",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.2",
    "@types/react": "18.0.15",
    "@types/react-virtualized-auto-sizer": "^1.0.1",
